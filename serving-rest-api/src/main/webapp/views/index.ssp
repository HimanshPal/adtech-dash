<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Classifier Metrics</title>


    <!-- 1. Add these JavaScript inclusions in the head of your page -->
    <script type="text/javascript" src="http://code.jquery.com/jquery-1.10.1.js"></script>
    <script type="text/javascript" src="http://code.highcharts.com/stock/highstock.js"></script>


    <!-- 2. Add the JavaScript to initialize the chart on document ready -->
    <script>
        $(function () {

        // Function for parsing JSON of classifierMetricsBundles
        function getMetric(data, metric) {
            var bundles = data["classifierMetricsBundles"];
            var result = [];
            var bundle;
            for (var i = 0; i < bundles.length; i++) {
                bundle = bundles[i];
                result.push([bundle["timestamp"], bundle[metric]]);
            }
            return result
        };

        $.getJSON(
            'http://ec2-52-90-198-107.compute-1.amazonaws.com:8081/classifier/metrics.json?since=1453183367829&until=1453187951889',
            function (data) {

                // Create the chart
                $('#container').highcharts('StockChart', {
                    rangeSelector : {
                    selected : 10
                },

                title : {
                    text : 'Classifier Metrics'
                },

                series : [{
                    name : 'Area Under Precision-Recall Curve',
                    data : getMetric(data, "auPRC"),
                    tooltip: {
                        valueDecimals: 4
                    }
                    }]
                });
            });
        });
		</script>

</head>
<body>

<!-- 3. Add the container -->
<div id="container" style="width: 800px; height: 400px; margin: 0 auto"></div>



</body>
</html>
